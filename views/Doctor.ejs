<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="doc.css">
    <title>Doctor Page</title>
</head>
<body onload="myfunct()">

    <section>
       
        <table class="container" >

            <h1><i>Hey Doctor <%= asd %> </i></h1>
            <thead>
                <tr>
                    <th><h1>First Name</h1></th>
                    <th><h1>Last Name</h1></th>
                    <th><h1>ID</h1></th>
                    <th><h1>E-mail</h1></th>
                    <th><h1>Gender</h1></th>
                    <th><h1>Age</h1></th>
                </tr>
            </thead>
            <tbody>
                <%for(var i=0; i<patientslist.length;i++){ %> 
                <tr>
                    <td><%= patientslist[i].FirstName%></td>
                    <td><%= patientslist[i].LastName%></td>
                    <td><%= patientslist[i].id%></td>
                    <td><%= patientslist[i].email%></td>
                    <td><%= patientslist[i].Gender%></td>
                    <td><%= patientslist[i].Age%></td>
                    <td><a href="Home.html"><button>View</button></a></td>
                </tr>
            <%}%>
            </tbody>
        </table>

    </section>

    </section>

    <!-- menu section ends -->
    <!-- location and navigation section start  -->

    <section class="location" id="location">

        <h1 class="heading"> <span>Navigate to our </span> Cafe</h1>

        <div class="row">

            <div class="mapouter">
                <div class="gmap_canvas">
                    <iframe width="600" height="500" id="gmap_canvas" src="https://maps.google.com/maps?q=rahat,zone%204&t=&z=13&ie=UTF8&iwloc=&output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe><a href="https://123movies-to.org">123movies</a><br>
                    <style>
                        .mapouter {
                            position: relative;
                            text-align: right;
                            height: 500px;
                            width: 600px;
                        }
                    </style><a href="https://www.embedgooglemap.net">use google maps on website</a>
                    <style>
                        .gmap_canvas {
                            overflow: hidden;
                            background: none !important;
                            height: 500px;
                            width: 600px;
                        }
                    </style>
                </div>
            </div>
            <div class="content">
                <h3>Our Address</h3>
                <p>Rahat,</p>
                <p>Zone 4, Main street</p>
                <p>We have Free parking slots for our customers !!</p>

            </div>

        </div>

    </section>
    
</body>
</html>